{
  "name": "pebble-log",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "dev": "APP_VARIANT=development pnpx expose-wsl@latest && expo start -c",
    "dev:tunnel": "APP_VARIANT=development expo start --tunnel -c",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "lint": "expo lint",
    "doctor": "pnpx expo-doctor",
    "build:dev": "eas build --platform android --profile development",
    "build:preview.android": "eas build --platform android --profile preview",
    "localDB:migrate": "drizzle-kit generate"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.722.0",
    "@aws-sdk/lib-storage": "^3.738.0",
    "@expo/vector-icons": "^14.0.2",
    "@op-engineering/op-sqlite": "^11.2.3",
    "@react-native-community/datetimepicker": "8.2.0",
    "@react-navigation/bottom-tabs": "^7.0.0",
    "@react-navigation/native": "^7.0.0",
    "@tanstack/react-query": "5.62.8",
    "@xstate/store": "^2.6.2",
    "babel-plugin-inline-import": "^3.0.0",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.39.3",
    "drizzle-zod": "^0.7.0",
    "expo": "~52.0.26",
    "expo-blur": "~14.0.3",
    "expo-constants": "~17.0.4",
    "expo-crypto": "~14.0.2",
    "expo-dev-client": "~5.0.10",
    "expo-device": "~7.0.2",
    "expo-file-system": "~18.0.7",
    "expo-font": "~13.0.3",
    "expo-haptics": "~14.0.1",
    "expo-linking": "~7.0.4",
    "expo-router": "~4.0.17",
    "expo-secure-store": "~14.0.1",
    "expo-splash-screen": "~0.29.21",
    "expo-status-bar": "~2.0.1",
    "expo-symbols": "~0.2.1",
    "expo-system-ui": "~4.0.7",
    "expo-web-browser": "~14.0.2",
    "nativewind": "^4.1.23",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.6",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-get-random-values": "^1.11.0",
    "react-native-mmkv": "^3.2.0",
    "react-native-reanimated": "~3.16.7",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-svg": "15.8.0",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "~0.19.13",
    "react-native-webview": "13.12.5",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "^3.4.16",
    "web-streams-polyfill": "^4.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/plugin-transform-class-static-block": "^7.26.0",
    "@types/jest": "^29.5.12",
    "@types/react": "~18.3.12",
    "@types/react-test-renderer": "^18.3.0",
    "drizzle-kit": "^0.29.1",
    "jest": "^29.2.1",
    "jest-expo": "~52.0.3",
    "react-test-renderer": "18.3.1",
    "typescript": "^5.3.3"
  },
  "private": true,
  "pnpm": {
    "patchedDependencies": {
      "drizzle-orm": "patches/drizzle-orm.patch"
    }
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "@xstate/store",
          "drizzle-orm",
          "drizzle-zod",
          "tailwindcss"
        ]
      }
    }
